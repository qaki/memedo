{
  "name": "memedo-backend",
  "version": "1.0.0",
  "description": "MemeDo Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "test:env": "tsx src/utils/test-env-validation.ts",
    "db:test": "tsx src/db/test-connection.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop"
  },
  "dependencies": {
    "@memedo/shared": "workspace:*",
    "@neondatabase/serverless": "^1.0.2",
    "@upstash/redis": "^1.35.6",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "otpauth": "^9.4.1",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "resend": "^6.4.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.24",
    "@types/qrcode": "^1.5.6",
    "drizzle-kit": "^0.31.6",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.20.0"
}
