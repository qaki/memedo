{
  "name": "@memedo/shared",
  "version": "1.0.0",
  "description": "Shared validation schemas, types, and utilities for MemeDo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "keywords": [
    "memedo",
    "validation",
    "zod",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.20.0"
}
